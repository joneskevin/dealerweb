<%@ page contentType="text/html; charset=utf-8"%>
<%@ include file="/pub/jstl.jsp"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<%@ include file="/dealer/include/meta.jsp"%>
</head>
<body>

	<div class="allContent">
		<table border="0" cellpadding="0" cellspacing="0" 
			class="tableBox">
			<tr>
				<td class="tdTitle">分销中心：</td>
				<td class="tdSpace">${company.parentName}</td>
			</tr>
			<tr>
				<td class="tdTitle">省份：</td>
				<td class="tdSpace">${company.regionProvinceId_Nick}</td>
			</tr>
			<tr>
				<td class="tdTitle">城市：</td>
				<td class="tdSpace">${company.regionCityId_Nick}</td>
			</tr>
			<tr>
				<td class="tdTitle">网络代码：</td>
				<td class="tdSpace">${company.dealerCode}</td>
			</tr>
			<tr>
				<td class="tdTitle">经销商名称：</td>
				<td class="tdSpace">${company.name}</td>
			</tr>
			<tr>
				<td class="tdTitle">经销商状态：</td>
				<td class="tdSpace">${company.isKeyCity_Nick}</td>
			</tr>
			<tr>
				<td class="tdTitle">网络形态：</td>
				<td class="tdSpace">${company.dealerType_Nick}</td>
			</tr>
			<tr>
				<td class="tdTitle">电话：</td>
				<td class="tdSpace">${company.contactTel}</td>
			</tr>
			<tr>
				<td class="tdTitle">传真：</td>
				<td class="tdSpace">${company.fax}</td>
			</tr>
			<tr>
				<td class="tdTitle">邮箱：</td>
				<td class="tdSpace">${company.email}</td>
			</tr>
			<tr>
				<td class="tdTitle">地址：</td>
				<td class="tdSpace">${company.contactAddress}</td>
			</tr>
			<tr>
				<td class="tdTitle">上班时间：</td>
				<td class="tdSpace">${company.startWorkTime_Nick}</td>
			</tr>
			<tr>
				<td class="tdTitle">下班时间：</td>
				<td class="tdSpace">${company.endWorkTime_Nick}</td>
			</tr>
			<tr>
				<td class="tdTitle" valign="top" >备注：</td>
				<td class="tdSpace">
				<form:textarea cssStyle="border:none; resize:none; background:#FFFFFF;" disabled="true" path="company.remark" rows="5" cols="40"  />
				</td>
			</tr>
			
		</table>
	</div>

</body>
</html>