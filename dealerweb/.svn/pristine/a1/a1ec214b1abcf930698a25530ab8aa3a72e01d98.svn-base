@charset "utf-8";
/* CSS Document */

body {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	background-color: #fafcfd;
}
.tip {
	padding-left: 2px;
	padding-top: 2px;
	padding-bottom: 5px;
	line-height: 130%;
	padding-right: 2px;
}


.tableBox {
	width: 100%;
	margin-bottom: 20px;
}
.tableBox th {
	line-height: 23px;
	background-image: url(../../images/tableBoxBg.gif);
	background-repeat: repeat-x;
	background-position: 0px 0px;
}
.tableBox td {
	padding: 5px;
}
.tableBox .tdTitle {
	background-color: #e4eef7;
	text-align: right;
	width: 110px;
	color: #1a405b;
}
.tableBox .top {
	vertical-align: top;
}
.tableBox .tdSpace {
	background-color: #FFFFFF;
	color: #666666;
	position: relative;
}
.tableBox .tdSpace textarea {
	width: 98%;
	font-size: 12px;
	color: #666666;
}
input.size   {
	width: 300px;
	height: 20px;
	color: #666666;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	margin-left: 0px;
}
input.date   {
	width: 100px;
}
.tableBox .tdSpace span {
	float: left;
	padding-right: 2px;
	height: 22px;
	overflow: hidden;
	margin: 0px;
}
.tableBox .tdSpace .normelStyle {
	float: none;
	padding-left: 5px;
}


.tableBox .tdSpace select {
	font-size: 12px;
	color: #666666;
}
.tableBox .tdSpace ol {
	padding-left: 15px;
	position: relative;
}


.tableBox .tdSpace li {
	list-style-type: decimal;
	margin-left: 5px;
}

.btn {
	text-align: center;
	border-top-width: 1px;
	border-top-style: solid;
	border-top-color: #b5cbdc;
	padding-top: 15px;
}
.btn input{
	padding: 5px;
}
	
.dialog {
	margin-bottom: 20px;
}

ul.linkList{ list-style:none;}
ul.linkList input{ vertical-align:middle;}
ul.linkList li{ line-height:30px; display:inline-block; width:200px;}